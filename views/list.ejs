<%- include('header') %>

<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>

<div class="box">
    <%for(var i = 0; i<itensList.length; i++){%>
      <div class="item">
        <input type="checkbox">
        <p><%= itensList[i] %></p>
      </div>
    <%}%>
</div>

<form class="" action="/" method="post">
  <input type="text" name="newItem" placeholder="New Item" autocomplete="off" autocapitalize="on"/>
  <button type="submit" name="button">+</button>
</form>

<%- include('footer') %>